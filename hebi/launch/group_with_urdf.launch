<launch>
  <param name="robot_description"
    command="$(find xacro)/xacro.py '$(find hebi)/urdf/x_demo.xacro'" />
  <param name="publish_frequency" value="50" />
  <node pkg="hebi" name="group_node" type="group_node" />
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="rob_st_pub" />
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find hebi)/hebi.rviz"/>
</launch>
